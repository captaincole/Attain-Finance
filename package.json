{
  "name": "personal-finance-mcp-server",
  "version": "1.0.0",
  "description": "Personal Finance MCP Server with Streamable HTTP Transport",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && shx chmod +x build/*.js",
    "watch": "tsc --watch",
    "start": "node build/index.js",
    "prepare": "npm run build",
    "dev": "concurrently \"npm run watch\" \"nodemon --watch build build/index.js\""
  },
  "files": [
    "build",
    "public",
    "src/prompts"
  ],
  "keywords": [
    "mcp",
    "personal-finance",
    "model-context-protocol"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@clerk/express": "^1.7.35",
    "@clerk/mcp-tools": "^0.3.1",
    "@modelcontextprotocol/sdk": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "plaid": "^39.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.10",
    "concurrently": "^8.2.2",
